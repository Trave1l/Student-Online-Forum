r; padding: 30px 0\">Welcome</div>\r\n      <el-form ref=\"form\" :model=\"form\" size=\"normal\" :rules=\"rules\">\r\n        <el-form-item prop=\"username\">\r\n          <el-input prefix-icon=\"el-icon-user-solid\" v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input prefix-icon=\"el-icon-lock\" v-model=\"form.password\" show-password></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button style=\"margin-left: 10px; width: 40%\" @click=\"$router.push('/register')\">注 册 </el-button>\r\n          <el-button style=\"margin-left: 30px;width: 42%\" type=\"primary\" @click=\"login\">Login</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data(){\r\n    return{\r\n      form: {},\r\n      rules:{\r\n        username: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户名',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            message: '请输入密码',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    login(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/api/user/login\", this.form).then(res =>{\r\n            console.log(res)\r\n            if(res.code == '0'){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登入成功\"\r\n              })\r\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data))  // 缓存用户信息\r\n              this.$router.push(\"/\")\r\n            }else{\r\n              this.$message({\r\n                type:\"error\",\r\n                message:res.msg\r\n              })\r\n            }\r\n          })\r\n        }//验证合法\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}