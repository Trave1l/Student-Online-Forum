   <el-table-column label=\"Êó∂Èó¥\" prop=\"time\" sortable/>\r\n      <el-table-column label=\"ÊµèËßàÊï∞\" prop=\"follow\" sortable/>\r\n      <el-table-column label=\"ÁÇπËµûÊï∞\" prop=\"good\" sortable/>\r\n      <el-table-column label=\"ÁÇπÁÅ≠Êï∞\" prop=\"bad\" sortable/>\r\n      <el-table-column label=\"Operations\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Jump(scope.row)\" v-if=\"scope.row.report == 0 || scope.row.report == 1\">\r\n            More\r\n          </el-button>\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Star(scope.row)\"  v-if=\" scope.row.report == 1 || this.user.role == 1 && scope.row.report == 0\">\r\n            Star\r\n          </el-button>\r\n          <el-button size=\"medium\" icon=\"el-icon-lollipop\" style=\"color: orangered\" type=\"text\" v-if=\"scope.row.star == 1 && scope.row.report == 0 || scope.row.report == 1\">\r\n          </el-button>\r\n          <el-button size=\"mini\" type=\"text\"  @click=\"\" style=\"color: red\" v-if=\"scope.row.report == 1\">\r\n            ËøùËßÑ\r\n          </el-button>\r\n          <el-button size=\"mini\" type=\"text\"  @click=\"\" style=\"color: darkblue\" v-if=\"scope.row.report == 2\">\r\n            Fraze\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    </div>\r\n\r\n    <el-dialog v-model=\"dialogVisible\" title=\"Êñ∞Â¢û\" width=\"50%\">\r\n      <el-form :model=\"form\" label-width=\"120px\">\r\n        <el-form-item label=\"Ê†áÈ¢ò\">\r\n          <el-input v-model=\"form.title\" style=\"width: 50%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"ÂàÜÂå∫\">\r\n          <template v-model=\"form.section\" #default=\"scope\">\r\n            <div>\r\n              <el-radio v-model=\"form.section\" label=\"Â≠¶‰π†\">Â≠¶‰π†</el-radio>\r\n              <el-radio v-model=\"form.section\" label=\"ÁæéÈ£ü\">ÁæéÈ£ü</el-radio>\r\n              <el-radio v-model=\"form.section\" label=\"ÊóÖÊ∏∏\">ÊóÖÊ∏∏</el-radio>\r\n              <el-radio v-model=\"form.section\" label=\"Ë¥≠Áâ©\">Ë¥≠Áâ©</el-radio>\r\n            </div>\r\n          </template>\r\n        </el-form-item>\r\n        <div id=\"div1\">\r\n        </div>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <!--    ÂàÜÈ°µ-->\r\n    <div class=\"demo-pagination-block\" style=\"margin: 10px 0; text-align: center\">\r\n      <el-pagination\r\n          v-model:currentPage=\"currentPage1\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <el-backtop :bottom=\"100\">\r\n      üöÄ\r\n    </el-backtop>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport E from \"wangeditor\";\r\nimport request from \"@/utils/request\";\r\n\r\nlet editor;\r\n\r\nexport default {\r\n  name: \"Bbs\",\r\n  data() {\r\n    return {\r\n      form:{},\r\n      dialogVisible: false,\r\n      search: '',\r\n      tableData: [],\r\n      loading: true,\r\n      notices: [],\r\n      currentPage1: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      active: 0,\r\n      vis:false,\r\n      tmp:{},\r\n    }\r\n  },\r\n  created() {\r\n    sessionStorage.removeItem(\"BBS\")\r\n    sessionStorage.removeItem(\"BbsId\")\r\n    this.user = sessionStorage.getItem(\"user\") ? JSON.parse(sessionStorage.getItem(\"user\")) : {};\r\n    this.load()\r\n  },\r\n  methods: {\r\n    Jump(row){\r\n      sessionStorage.setItem(\"BBS\", JSON.stringify(row))\r\n      sessionStorage.setItem(\"BbsId\", row.id)\r\n      this.$router.push('/details')\r\n      this.user = sessionStorage.getItem(\"user\") ? JSON.parse(sessionStorage.getItem(\"user\")) : {};\r\n      request.put(\"/api/bbs/\", row)\r\n      request.put(\"/api/message/\", row)\r\n      request.put(\"/api/store/\", this.tmp)\r\n    },\r\n    load() {\r\n      this.loading = true\r\n      this.form = {}\r\n      request.get(\"/api/bbs/page\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        this.loading = false\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    add() {\r\n      this.dialogVisible = true\r\n      this.$nextTick(() => {\r\n        // ÂÖ≥ËÅîÂºπÁ™óÈáåÈù¢ÁöÑdivÔºånew‰∏Ä‰∏™ editorÂØπË±°\r\n        if (!editor) {\r\n          editor = new E('#div1')\r\n          editor.config.uploadImgServer = \"http://localhost:9092/files/editor/upload\"\r\n          editor.config.uploadFileName = 'file'\r\n          editor.create()\r\n        }\r\n        editor.txt.html(\"\")\r\n      })\r\n    },\r\n    save(){\r\n      this.form.content = editor.txt.html()//Ëé∑ÂèñÁºñËæëÂô®ÁöÑÂÜÖÂÆπÔºåÂ§çÂà∂ --> ËΩ¨Â≠ò\r\n\r\n      let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n      let user = JSON.parse(userStr)\r\n\r\n      this.form.author = user.nickname\r\n      this.form.aid = user.id\r\n      this.form.follow = 1\r\n      request.post(\"/api/bbs/\", this.form).then(res =>{\r\n        if(res.code == '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"Êñ∞Â¢ûÊàêÂäü\"\r\n          })\r\n          this.load()//Âà∑Êñ∞Êï∞ÊçÆ\r\n        }else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n        this.dialogVisible = false\r\n        })\r\n    },\r\n    handleSizeChange(pageSize){//ÊîπÂèòÂΩìÂâç‰∏™Êï∞\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){//ÊîπÂèòÂΩìÂâçÈ°µÁ†Å\r\n      this.currentPage1 = pageNum\r\n      this.load()\r\n    },\r\n    Star(row) {\r\n      request.put(\"/api/bbs/star\", row).then(res => {\r\n        if (res.code == '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"ÊàêÂäü\"\r\n          })\r\n          this.load()\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.table /deep/ .el-table th {\r\n  background-color: transparent!important;\r\n}\r\n\r\n.table{\r\n  background: url(\"../assets/back1.jpg\") no-repeat;\r\n  background-size: 100% 100%;\r\n}\r\n\r\n.table /deep/ .el-table tr {\r\n  background-color: transparent!important;\r\n}\r\n.table /deep/  .el-table--enable-row-transition .el-table__body td, .el-table .cell{\r\n  background-color: transparent;\r\n}\r\n.el-table::before {\r\n  left: 0;\r\n  bottom: 0;\r\n  width: 100%;\r\n  height: 0px;\r\n}\r\n</style>"]}]}