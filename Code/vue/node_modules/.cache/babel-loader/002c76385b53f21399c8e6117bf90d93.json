\">\r\n      <el-table-column label=\"标题\" prop=\"title\" sortable/>\r\n      <el-table-column label=\"作者\" prop=\"author\" />\r\n      <el-table-column label=\"时间\" prop=\"time\" sortable/>\r\n      <el-table-column label=\"浏览数\" prop=\"follow\" sortable/>\r\n      <el-table-column label=\"Operations\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Jump(scope.row)\">\r\n            More\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Index\",\r\n  data(){\r\n    return {\r\n      form:{},\r\n      tableData:[],\r\n      TableVisible:false,\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n    this.bbs()\r\n  },\r\n  methods:{\r\n    bbs(){\r\n      let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n      let tmp = JSON.parse(str)\r\n      request.get(\"/api/bbs/ubbs/\" + tmp.id).then(res=>{\r\n        this.tableData = res.data\r\n      })\r\n    },\r\n    cmt(){\r\n      let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n      let tmp = JSON.parse(str)\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}