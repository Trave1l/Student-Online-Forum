,MAAiB;AAAV,YAAA,QAAQ,EAAR;AAAU,WAAlD,CADmD,EAEnD,YAAA,CAA4D,0BAA5D,EAA4D;AAA3C,YAAA,KAAK,EAAC,IAAqC;AAAhC,YAAA,IAAI,EAAC,gBAA2B;AAAV,YAAA,QAAQ,EAAR;AAAU,WAA5D,CAFmD,EAGnD,YAAA,CAWkB,0BAXlB,EAWkB;AAXD,YAAA,KAAK,EAAC;AAWL,WAXlB,EAAmC;AACtB,YAAA,OAAO,EAAA,QAAA,CAChB,UADuB,KACvB;AAAA,qBADuB,CACvB,YAAA,CAEY,oBAFZ,EAEY;AAFD,gBAAA,IAAI,EAAC,MAEJ;AAFW,gBAAA,IAAI,EAAC,MAEhB;AAFwB,gBAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,yBAAE,QAAA,CAAA,IAAA,CAAK,KAAK,CAAC,GAAX,CAAF;AAAA;AAE7B,eAFZ,E;kCAA4D;AAAA,yBAE5D,C,WAAA,CAF4D;AAAA,iB;;;;eAA5D,E;;AAAA,gB,WAAA,CADuB,EAIvB,YAAA,CAIgB,wBAJhB,EAIgB;AAJD,gBAAA,IAAI,EAAC,QAIJ;AAJa,gBAAA,KAAK,EAAC,8BAInB;AAJmD,gBAAA,SAAO,EAAA,mBAAA,MAAA;AAAA,yBAAE,QAAA,CAAA,YAAA,CAAa,KAAK,CAAC,GAAN,CAAU,EAAvB,CAAF;AAAA;AAI1D,eAJhB,E;AACa,gBAAA,SAAS,EAAA,QAAA,CAClB;AAAA,yBAAiD,CAAjD,YAAA,CAAiD,oBAAjD,EAAiD;AAAtC,oBAAA,IAAI,EAAC,MAAiC;AAA1B,oBAAA,IAAI,EAAC;AAAqB,mBAAjD,E;sCAAmC;AAAA,6BAAE,C,WAAA,CAAF;AAAA,qB;;;;mBAAnC,CAAiD,CAAjD;AAAA,iBADkB,C;;;;eADtB,E;;AAAA,gB,aAAA,CAJuB,CACvB;AAAA,aADgB,CADe;;;;AAAA,WAAnC,CAHmD,CAAnD;AAAA,S;;;;OALF,E;;AAAA,Q,sBAAA,CAXU,CAHV;AAAA,K,CADwC;;;;AAAA,GAA1C,C","sourcesContent":["<template>\r\n  <el-card style=\"width: 40%; margin: 10px;\">\r\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\r\n      <el-image :src=\"form.avatar\" style=\"width: 100px; height: 100px; border-radius: 60%; margin-left: 45%\"></el-image>\r\n      <div style=\"text-align: center; font-size: x-large;font-weight: bolder\">{{form.nickname}}</div>\r\n    </el-form>\r\n    <div style=\"text-align: center; font-size: large; font-family: 'Book Antiqua'\">\r\n      <span v-if=\"form.role === 1\">管理员</span>\r\n      <span v-if=\"form.role === 2\">普通用户</span>\r\n      <span v-if=\"form.role === 3\">房管</span>\r\n    </div><div>&nbsp;</div><div>&nbsp;</div>\r\n    <div style=\"margin-left: 37%\">\r\n      <el-button @click=\"$router.push('/index')\" type=\"text\" style=\"font-size: large; margin-left: 20px\">发帖记录</el-button>\r\n      <el-button @click=\"cmt\" type=\"text\" style=\"font-size: large; margin-left: 20px\">评论记录</el-button>\r\n      <el-button @click=\"$router.push('/IndexStore')\" type=\"text\" style=\"font-size: large; margin-left: 20px\">收 藏</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\"\r\n              border\r\n              stripe\r\n              style=\"width: 100%\"\r\n              v-model=\"TableVisible\">\r\n      <el-table-column label=\"标题\" prop=\"title\" sortable/>\r\n      <el-table-column label=\"时间\" prop=\"time\" sortable/>\r\n      <el-table-column label=\"评论\" prop=\"messageContent\" sortable/>\r\n      <el-table-column label=\"Operations\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Jump(scope.row)\">\r\n            More\r\n          </el-button>\r\n          <el-popconfirm type=\"danger\" title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\">\r\n            <template #reference>\r\n              <el-button size=\"mini\" type=\"text\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Index\",\r\n  data(){\r\n    return {\r\n      form:{},\r\n      tableData:[],\r\n      TableVisible:false,\r\n      tmp:{},\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n    this.cmt()\r\n  },\r\n  methods:{\r\n    cmt(){\r\n      let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n      let tmp = JSON.parse(str)\r\n\r\n      request.get(\"/api/message/ucmt/\" + tmp.id).then(res=>{\r\n        this.tableData = res.data\r\n      })\r\n    },\r\n    Jump(row){\r\n      sessionStorage.setItem(\"BBS\", JSON.stringify(row))\r\n      sessionStorage.setItem(\"BbsId\", row.foreignId)\r\n      this.$router.push('/details')\r\n      // this.tmp.aid = row.aid\r\n      // this.tmp.author = row.author\r\n      // this.tmp.follow = row.follow\r\n      // this.tmp.bbsContent = row.content\r\n      // this.tmp.id = row.foreignId\r\n      // this.tmp.title = row.title\r\n      request.put(\"/api/bbs/\", row)\r\n    },\r\n    handleDelete(id){\r\n      request.delete(\"/api/message/\" + id).then(res =>{\r\n        if(res.code == '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"删除成功\"\r\n          })\r\n          this.cmt()//刷新数据\r\n        }else{\r\n          this.$message({\r\n            type:\"success\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}