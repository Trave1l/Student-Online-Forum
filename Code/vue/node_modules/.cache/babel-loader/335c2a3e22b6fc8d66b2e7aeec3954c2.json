u-item>\r\n    </el-sub-menu>\r\n    <el-menu-item index=\"/bms\">\r\n      📰 公告\r\n    </el-menu-item>\r\n    <el-sub-menu >\r\n      <template #title >\r\n        📚 论坛</template>\r\n      <el-menu-item index=\"/bbs\" >\r\n        📖 首页\r\n      </el-menu-item>\r\n      <el-menu-item index=\"/bbsStudy\" >\r\n        📕 学习\r\n      </el-menu-item>\r\n      <el-menu-item index=\"/bbsFood\">\r\n        🍽️ 美食</el-menu-item>\r\n      <el-menu-item index=\"/bbsShop\">\r\n        🛒 购物</el-menu-item>\r\n      <el-menu-item index=\"/bbsTravel\">\r\n        🚢 旅游</el-menu-item>\r\n    </el-sub-menu>\r\n  </el-menu>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Aside\",\r\n  data(){\r\n    return{\r\n      user:{},\r\n      path:this.$route.path,//设置默认高亮\r\n      isCollapse:true,\r\n    }\r\n  },\r\n  created() {\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n\r\n    // // 请求服务端，确认当前登录用户的 合法信息\r\n    request.get(\"/api/user/\" + this.user.id).then(res => {\r\n      if (res.code === '0') {\r\n        this.user = res.data\r\n      }\r\n    })\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":""}]}