     <el-table-column label=\"Ê†áÈ¢ò\" prop=\"title\" sortable/>\r\n      <el-table-column label=\"‰ΩúËÄÖ\" prop=\"author\" />\r\n      <el-table-column label=\"Êó∂Èó¥\" prop=\"time\" sortable/>\r\n      <el-table-column label=\"ÊµèËßàÊï∞\" prop=\"follow\" sortable/>\r\n      <el-table-column label=\"Operations\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"text\" @click=\"details(scope.row)\">\r\n            ËØ¶ÊÉÖ\r\n          </el-button>\r\n          <el-popconfirm type=\"danger\" title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\" v-if=\"user.role == 1\">\r\n            <template #reference>\r\n              <el-button size=\"mini\" type=\"text\">Âà†Èô§</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Star(scope.row)\">\r\n            Âä†Á≤æ\r\n          </el-button>\r\n          <el-button size=\"medium\" icon=\"el-icon-lollipop\" style=\"color: orangered\" type=\"text\" v-if=\"scope.row.star == 1\">\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!--    ÂàÜÈ°µ-->\r\n    <div class=\"demo-pagination-block\" style=\"margin: 10px 0; text-align: center\">\r\n      <el-pagination\r\n          v-model:currentPage=\"currentPage1\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog v-model=\"dialogVisible\" title=\"Êñ∞Â¢û\" width=\"50%\">\r\n      <el-form :model=\"form\" label-width=\"120px\">\r\n        <el-form-item label=\"Ê†áÈ¢ò\">\r\n          <el-input v-model=\"form.title\" style=\"width: 50%\"></el-input>\r\n        </el-form-item>\r\n        <div id=\"div1\">\r\n        </div>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-model=\"vis\" title=\"ÈÄöÂëä\" width=\"50%\">\r\n      <el-card>\r\n        <div v-html=\"detail.content\" style=\"min-height: 200px\"></div>\r\n      </el-card>\r\n    </el-dialog>\r\n  </div>\r\n  <el-backtop :bottom=\"100\">\r\n    üöÄ\r\n  </el-backtop>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport E from \"wangeditor\";\r\n\r\nlet editor\r\n\r\nexport default {\r\n  name: \"Bbs_Admin\",\r\n  data() {\r\n    return {\r\n      form:{\r\n      },\r\n      loading: true,\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage1: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData: [],\r\n      detail:{},\r\n      vis:false,\r\n      user:{},\r\n    }\r\n  },\r\n  components: {},\r\n  created() {\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    // // ËØ∑Ê±ÇÊúçÂä°Á´ØÔºåÁ°ÆËÆ§ÂΩìÂâçÁôªÂΩïÁî®Êà∑ÁöÑ ÂêàÊ≥ï‰ø°ÊÅØ\r\n    request.get(\"/api/user/\" + this.user.id).then(res => {\r\n      if (res.code === '0') {\r\n        this.user = res.data\r\n      }\r\n    })\r\n    this.load();\r\n  },\r\n  methods:{\r\n    details(row) {\r\n      this.vis = true,\r\n          this.detail = row\r\n    },\r\n    Star(row) {\r\n      request.put(\"/api/bbs/star\", row).then(res => {\r\n        if (res.code == '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"ÊàêÂäü\"\r\n          })\r\n          this.load()\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    fileUploadSuccess(res){\r\n      this.form.avatar = res.data\r\n    },\r\n    load() {\r\n      this.loading = true\r\n      request.get(\"/api/bbs/page\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        this.loading = false\r\n        this.tableData = res.data.records.reverse()\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save(){\r\n      this.form.content = editor.txt.html()//Ëé∑ÂèñÁºñËæëÂô®ÁöÑÂÜÖÂÆπÔºåÂ§çÂà∂ --> ËΩ¨Â≠ò\r\n\r\n      if(this.form.id){//Êõ¥Êñ∞\r\n        this.dialogVisible = false\r\n        request.put(\"/api/bbs/\", this.form).then(res =>{\r\n          if(res.code == '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"Êõ¥Êñ∞ÊàêÂäü\"\r\n            })\r\n            this.load()\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()//Âà∑Êñ∞Êï∞ÊçÆ\r\n          this.$refs['upload'].clearFiles()//Ê∏ÖÈô§Êñá‰ª∂ÂàóË°®\r\n          this.dialogVisible = false\r\n        })\r\n      }//Êõ¥Êñ∞\r\n      else{\r\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n        let user = JSON.parse(userStr)\r\n\r\n        this.form.author = user.nickname\r\n        request.post(\"/api/bbs/\", this.form).then(res =>{\r\n          if(res.code == '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"Êñ∞Â¢ûÊàêÂäü\"\r\n            })\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()//Âà∑Êñ∞Êï∞ÊçÆ\r\n          this.dialogVisible = false\r\n        })\r\n      }//Êñ∞Â¢û\r\n    },\r\n    handleEdit(row){\r\n      this.form = JSON.parse((JSON.stringify(row)))\r\n      this.dialogVisible = true\r\n      this.$nextTick(() => {\r\n        // ÂÖ≥ËÅîÂºπÁ™óÈáåÈù¢ÁöÑdivÔºånew‰∏Ä‰∏™ editorÂØπË±°\r\n        if (!editor) {\r\n          editor = new E('#div1')\r\n\r\n          editor.config.uploadImgServer = \"http://localhost:9092/files/editor/upload\"\r\n          editor.config.uploadFileName = 'file'\r\n          // ÈÖçÁΩÆ server Êé•Âè£Âú∞ÂùÄ\r\n          editor.create()\r\n        }\r\n        editor.txt.html(row.content)\r\n      })\r\n    },\r\n    handleDelete(id){\r\n      console.log(id)\r\n      request.delete(\"/api/bbs/\" + id).then(res =>{\r\n        if(res.code == '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"Âà†Èô§ÊàêÂäü\"\r\n          })\r\n        }else{\r\n          this.$message({\r\n            type:\"success\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n      this.load()//Âà∑Êñ∞Êï∞ÊçÆ\r\n    },\r\n    handleSizeChange(pageSize){//ÊîπÂèòÂΩìÂâç‰∏™Êï∞\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){//ÊîπÂèòÂΩìÂâçÈ°µÁ†Å\r\n      this.currentPage1 = pageNum\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}