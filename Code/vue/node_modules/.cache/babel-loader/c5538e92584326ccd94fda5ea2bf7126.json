;AAAA,aAOU,CAPV,YAAA,CAOU,kBAPV,EAOU;AAPA,QAAA,KAAK,EAAE,KAAA,CAAA,IAOP;AAPa,uBAAY;AAOzB,OAPV,E;0BACE;AAAA,iBAEe,CAFf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAA6D,CAA7D,YAAA,CAA6D,mBAA7D,EAA6D;4BAA1C,KAAA,CAAA,IAAA,CAAK,KAAqC;;yBAA1C,KAAA,CAAA,IAAA,CAAK,K,GAAK,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA7D,E,IAAA,E;;AAAA,gB,cAAA,CAA6D,CAA7D;AAAA,a,CADsB;;;;AAAA,WAAxB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC,IAEL;AAFU,2BAAY;AAEtB,WAFf,E;8BACE;AAAA,qBAA4G,CAA5G,YAAA,CAA4G,mBAA5G,EAA4G;AAAlG,gBAAA,IAAI,EAAC,UAA6F;AAAjF,gBAAA,IAAI,EAAE,CAA2E;4BAA/D,KAAA,CAAA,IAAA,CAAK,OAA0D;;yBAA/D,KAAA,CAAA,IAAA,CAAK,O,GAAO,M;kBAAmD;AAAjD,gBAAA,YAAY,EAAC,KAAoC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA5G,E,IAAA,E;;AAAA,gB,cAAA,CAA4G,CAA5G;AAAA,a;;;;WADF,CADe,CAFf;AAAA,S;;;;OADF,E;;AAAA,Q,SAAA,CAOU,CAPV;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,CAgBI,CAzCN,C","sourcesContent":["<template>\r\n  <div style=\"padding-left: 10px\">\r\n    <div style=\"padding: 5px 0\">\r\n      <el-button @click=\"add\" type=\"primary\" size=\"mini\" style=\"margin: 10px 0\">新增</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"100\"> </el-table-column>\r\n      <el-table-column prop=\"title\" label=\"标题\"> </el-table-column>\r\n      <el-table-column prop=\"content\" label=\"内容\"> </el-table-column>\r\n      <el-table-column prop=\"time\" label=\"发布时间\"> </el-table-column>\r\n\r\n      <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" icon=\"el-icon-edit\" circle  @click=\"edit(scope.row)\"></el-button>\r\n          <el-popconfirm\r\n              @confirm=\"del(scope.row.id)\"\r\n              title=\"确定删除？\">\r\n            <el-button type=\"danger\" icon=\"el-icon-delete\" circle slot=\"reference\" style=\"margin-left: 10px\"></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog  v-model=\"dialogVisible\" title=\"新公告\" width=\"50%\">\r\n      <el-form :model=\"form\" label-width=\"120px\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"form.title\" style=\"width: 50%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\" label-width=\"120px\">\r\n          <el-input type=\"textarea\" :rows=\"5\" v-model=\"form.content\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"AddBms\",\r\n  data() {\r\n    return {\r\n      form:{\r\n      },\r\n      loading: true,\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage1: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData: [],\r\n      detail:{},\r\n      vis:false,\r\n      user:{},\r\n    }\r\n  },\r\n  created() {\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    // // 请求服务端，确认当前登录用户的 合法信息\r\n    request.get(\"/api/user/\" + this.user.id).then(res => {\r\n      if (res.code === '0') {\r\n        this.user = res.data\r\n      }\r\n    })\r\n    this.load();\r\n  },\r\n  methods:{\r\n    add(){\r\n      this.dialogVisible = true\r\n    },\r\n    save(){\r\n      if(this.form.id){//更新\r\n        this.dialogVisible = false\r\n        request.put(\"/api/board/\", this.form).then(res =>{\r\n          if(res.code == '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()//刷新数据\r\n          this.$refs['upload'].clearFiles()//清除文件列表\r\n          this.dialogVisible = false\r\n        })\r\n      }//更新\r\n      else{\r\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n        let user = JSON.parse(userStr)\r\n\r\n        this.form.author = user.nickname\r\n        request.post(\"/api/board/\", this.form).then(res =>{\r\n          if(res.code == '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"新增成功\"\r\n            })\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()//刷新数据\r\n          this.dialogVisible = false\r\n        })\r\n      }//新增\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}