AAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,IAArB;AACD,OAHD;AAID,KAVK;AAWN,IAAA,IAXM,gBAWD,GAXC,EAWG;AACP,MAAA,GAAG,CAAC,IAAJ,GAAW,CAAX;AACA,MAAA,GAAG,CAAC,GAAJ,GAAU,CAAV;AACA,MAAA,cAAc,CAAC,OAAf,CAAuB,KAAvB,EAA8B,IAAI,CAAC,SAAL,CAAe,GAAf,CAA9B;AACA,MAAA,cAAc,CAAC,OAAf,CAAuB,OAAvB,EAAgC,GAAG,CAAC,KAApC;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB,UAAlB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,WAAZ,EAAyB,GAAzB;AACD,KAlBK;AAmBN,IAAA,YAnBM,wBAmBO,EAnBP,EAmBU;AAAA;;AACd,MAAA,OAAO,CAAC,MAAR,CAAe,gBAAgB,EAA/B,EAAmC,IAAnC,CAAwC,UAAA,GAAE,EAAI;AAC5C,YAAG,GAAG,CAAC,IAAJ,IAAY,GAAf,EAAmB;AACjB,UAAA,MAAI,CAAC,QAAL,CAAc;AACZ,YAAA,IAAI,EAAC,SADO;AAEZ,YAAA,OAAO,EAAC;AAFI,WAAd;;AAIA,UAAA,MAAI,CAAC,IAAL,GALiB,CAKN;;AACZ,SAND,MAMK;AACH,UAAA,MAAI,CAAC,QAAL,CAAc;AACZ,YAAA,IAAI,EAAC,SADO;AAEZ,YAAA,OAAO,EAAC,GAAG,CAAC;AAFA,WAAd;AAIF;AACD,OAbD;AAcD;AAlCK;AAhBK,CAAf","sourcesContent":["<template>\r\n  <div class=\"table\">\r\n    <div class=\"bgImg\" style=\"text-align: center\">\r\n  <el-card style=\"width: 96%; margin: 20px; text-align: center; background: rgba(0,0,0,0)\">\r\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\r\n      <el-image :src=\"form.avatar\" style=\"width: 100px; height: 100px; border-radius: 60%;\"></el-image>\r\n      <div style=\"text-align: center; font-size: x-large;font-weight: bolder;font-family: fantasy\">{{form.nickname}}</div>\r\n    </el-form>\r\n    <div style=\"text-align: center; font-size: large; font-family: 'Book Antiqua'\">\r\n      <span v-if=\"form.role === 1\" style=\"color: #f24b17\">Admin</span>\r\n      <span v-if=\"form.role === 2\">Ordinary</span>\r\n    </div><div>&nbsp;</div><div>&nbsp;</div>\r\n    <div>\r\n      <el-button @click=\"$router.push('/index')\" type=\"text\" style=\"font-size: large; margin-left: 20px\">BBS Records</el-button>\r\n      <el-button @click=\"$router.push('/IndexComment')\" type=\"text\" style=\"font-size: large; margin-left: 20px\">Comment Records</el-button>\r\n      <el-button @click=\"StoreBbs\" type=\"text\" style=\"font-size: large; margin-left: 20px\">Store</el-button>\r\n    </div>\r\n    <br />\r\n    <el-table :data=\"tableData\"\r\n              border\r\n              style=\"width: 100%; background-color: rgba(0,0,0,0); color: black\"\r\n              v-model=\"TableVisible\"\r\n              class=\"table\">\r\n      <el-table-column label=\"Title\" prop=\"title\" sortable/>\r\n      <el-table-column label=\"Time\" prop=\"time\" sortable/>\r\n      <el-table-column label=\"Operations\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"text\" @click=\"Jump(scope.row)\">\r\n            More\r\n          </el-button>\r\n          <el-popconfirm type=\"danger\" title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\">\r\n            <template #reference>\r\n              <el-button size=\"mini\" type=\"text\">Delete</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Index\",\r\n  data(){\r\n    return {\r\n      form:{},\r\n      tableData:[],\r\n      TableVisible:false,\r\n    }\r\n  },\r\n  created() {\r\n    sessionStorage.removeItem(\"BBS\")\r\n    sessionStorage.removeItem(\"BbsId\")\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      this.StoreBbs()\r\n    },\r\n    StoreBbs(){\r\n      let user = JSON.parse(sessionStorage.getItem(\"user\"))\r\n      request.get(\"/api/store/bbsUID/\" + user.id).then(res=>{\r\n        console.log(res.data)\r\n        this.tableData = res.data\r\n      })\r\n    },\r\n    Jump(row){\r\n      row.good = 0;\r\n      row.bad = 0;\r\n      sessionStorage.setItem(\"BBS\", JSON.stringify(row))\r\n      sessionStorage.setItem(\"BbsId\", row.bbsId)\r\n      this.$router.push('/details')\r\n      request.put(\"/api/bbs/\", row)\r\n    },\r\n    handleDelete(id){\r\n      request.delete(\"/api/store/\" + id).then(res =>{\r\n        if(res.code == '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"删除成功\"\r\n          })\r\n          this.load()//刷新数据\r\n        }else{\r\n          this.$message({\r\n            type:\"success\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.bgImg{\r\n  background: url(\"../assets/img_4.png\") no-repeat;\r\n  background-size: 100% 100%;\r\n  background-opacity: 0.5;\r\n}\r\n.table /deep/ .el-table th {\r\n  background-color: transparent!important;\r\n}\r\n\r\n.table{\r\n  background: url(\"../assets/img_3.png\") no-repeat;\r\n  background-size: 100% 100%;\r\n}\r\n.el-table el-table-column{\r\n  color: black;\r\n}\r\n\r\n.table /deep/ .el-table tr {\r\n  background-color: transparent!important;\r\n}\r\n.table /deep/  .el-table--enable-row-transition .el-table__body td, .el-table .cell{\r\n  background-color: transparent;\r\n}\r\n</style>"],"sourceRoot":""}]}